<%- include ("./partials/header") %>
<div class="banner-area jarallax" style="background-image: url(assets/img/bg/test2.webp);" id="bg-img-top">
    <div class="container">
        <div class="banner-inner-wrap">
            <div class="row">
                <div class="col-12" >
                    <div class="banner-inner">
                        <h5 class="sub-title"></h5>
                        <h1 class="title">Suntem aici pentru a-ți face experiența cumpărării unui imobil cât mai puțin stresantă.</h1>
                    </div>
                </div>
                <div class="col-12">
                    <div class="banner-search-wrap">
                        <div class="tab-content">
                            <div class="tab-pane fade show active" id="tabs_1">
                                <div class="rld-main-search">
                                    <form type="search" action="/" method="POST" name="searchForm">
                                    <div class="row justify-content-center">
                                        
                                        <div class="col-xl-2 col-lg-4 col-md-4">
                                            <div class="rld-single-select">
                                                <select class="select single-select" name="transactionType">
                                                    <option id="transactionTypeundefined" value="" selected disabled hidden>Tip Tranzacție</option>  
                                                    <option id="transactionType1" value="1">Vânzare</option>
                                                    <option id="transactionType2" value="2">Închiriere</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-xl-2 col-lg-6 col-md-6">
                                            <div class="rld-single-select">
                                                <select class="select single-select" name="propType">
                                                    <option id="propType1" value="1, 3, 4, 5, 6, 7">Tip Imobil</option>
                                                    <option id="propType2" value="1">Apartament</option>
                                                    <option id="propType3" value="3">Casă</option>
                                                    <option id="propType4" value="4, 5, 7">Spațiu Comercial</option>
                                                    <option id="propType5" value="6">Teren</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-xl-2 col-lg-4 col-md-4">
                                            <div class="rld-single-select">
                                                <select class="select single-select" name="minRooms">
                                                  <option value="">Min. Cam</option>
                                                  <option value="1">1</option>
                                                  <option value="2">2</option>
                                                  <option value="3">3</option>
                                                  <option value="4">4</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-xl-2 col-lg-4 col-md-4">
                                            <div class="rld-single-select">
                                                <select class="select single-select" name="maxRooms">
                                                  <option value="">Max. Cam</option>
                                                  <option value="1">1</option>
                                                  <option value="2">2</option>
                                                  <option value="3">3</option>
                                                  <option value="4">4</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-xl-2 col-lg-4 col-md-4">
                                            <div class="rld-single-select">
                                                    <a class="btn btn-orange area-btn" data-toggle="collapse" href="#fullAreas" role="button" aria-expanded="false" aria-controls="fullAreas" >
                                                      Alege Zone
                                                    </a>
                                            </div>
                                        </div>
                                        <div class="col-xl-2 col-lg-4 col-md-4">
                                            <button type="submit" name="submit" class="btn btn-yellow" href="" >Caută  <i class="fa fa-search" ></i>
                                        </div>
                                    </div>
                                    <div class="collapse" id="fullAreas" >
                                        <div class="card card-body">
                                            <div class="row">
                                            
                                                <% var itemStart = 0%>
                                                <% for (let n= 1; n < areaList.length/10 +1; n++) {  %>
                                                
                                                    <div class="col-sm-6 col-md-4 col-lg-2" >
                                                
                                                
                                                    <% for (let count = itemStart ; count < n*10; count++){ %>
                                                        <% if ( count >= areaList.length) { %>
                                                            <% break %>
                                                         <% } %>
                                                         <label for=<%= areaList[count]%> class="checkbox" style="display: block;">
                                                            <input type="checkbox" class="area" id="zona <%= areaList[count]%>" name="area-cbx"  value="<%= areaList[count]%>" >
                                                            <%= areaList[count] %>
                                                            </label>
                                                    <% } %>
                                                    </div>  
                                                    <% itemStart += 10%>
                                                <% } %>
                                            
                                        </div>
                                        <div class="fixed-bottom-mobile">
                                        <a class="btn btn-yellow" data-toggle="collapse" href="#fullAreas" role="button" aria-expanded="false" aria-controls="fullAreas">
                                            Închide
                                          </a>
                                        </div>
                                      </div>
                                </div>
                            </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- banner area end -->




<!-- client area start -->
<div class="why-choose-us-area bg-gray pd-top-90 pd-bottom-60">
<div class="client-area pd-top-92 pd-bottom-100">
    <div class="container">
        <div class="section-title text-center">
            <h2 class="title">Parteneri</h2>
        </div>
        
        <div class="row partner-slider ">
            <div class="col-md-4">
                <div class="thumb text-center">
                    <img src="assets/img/client/generali.webp" loading="lazy" alt="client" class="partners">
                </div>
            </div>
            <div class="col-md-4">
                <div class="thumb text-center">
                    <img src="assets/img/client/kiwi.webp" loading="lazy" alt="client"class="partners">
                </div>
            </div>
            <div class="col-md-4">
                <div class="thumb text-center">
                    <img src="assets/img/client/otp.webp" loading="lazy" alt="client" class="partners" >
                </div>
            </div>
            
        </div>
    </div>
</div>
</div>
<%- include ("./partials/footer") %>
